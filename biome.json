{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"vcs": {
		"enabled": false,
		"clientKind": "git",
		"useIgnoreFile": false
	},
	"files": {
		"ignoreUnknown": false,
		"ignore": ["node_modules", "sampleWorkspace", "dist", "out", "coverage", ".vscode-test"]
	},
	"formatter": {
		"enabled": true,
		"indentStyle": "space"
	},
	"organizeImports": {
		"enabled": true,
		"include": ["**/*.ts", "**/*.js"]
	},
	"linter": {
		"enabled": true,
		"rules": {
			"all": true,
			"complexity": {
				"noForEach": "off"
			},
		}
	},
	"json": {
		"assists": {
			"enabled": true
		},
		"formatter": {
			"enabled": true,
			"trailingCommas": "none"
		},
		"linter": {
			"enabled": true
		},
		"parser": {
			"allowComments": false,
			"allowTrailingCommas": false
		}
	}
}
